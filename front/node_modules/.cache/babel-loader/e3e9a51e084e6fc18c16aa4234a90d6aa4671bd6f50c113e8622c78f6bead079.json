e=\"primary\" plain @click=\"openTopic\">预览课题</el-button>\r\n                    <el-button type=\"success\" plain @click=\"topicSelect\">进入选题</el-button>\r\n                </el-row>\r\n            </div>\r\n        </template>\r\n\r\n        <!-- 预览课题页面 -->\r\n        <el-dialog title=\"预览课题页面\" :visible.sync=\"previewDialogVisible\" width=\"80%\">\r\n            <span>这是预览课题页面</span>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"TopicTask\",\r\n    data() {\r\n        return {\r\n            hasTask: false,\r\n            year: 3022,\r\n            hasTopic: false,\r\n            taskName: \"选题任务名1\",\r\n            previewDialogVisible: false,\r\n            // 时间线\r\n            reverse: true,\r\n            activities: [{\r\n                content: '活动按期开始',\r\n                timestamp: '2018-04-15'\r\n            }, {\r\n                content: '通过审核',\r\n                timestamp: '2018-04-13'\r\n            }, {\r\n                content: '创建成功',\r\n                timestamp: '2018-04-11'\r\n            }]\r\n        }\r\n    },\r\n    methods: {\r\n        topicSelect() {\r\n            this.$router.push(\"/home/student/topicSelect\")\r\n        },\r\n        openTopic() {\r\n            // 发送axios 请求 \r\n            // 根据 学生的专业  获取所有课题\r\n            this.previewDialogVisible = true\r\n        }\r\n\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}