    if (this.$props.type == \"0\") {\r\n                this.filename = \"json/studentMenu.json\"\r\n            } else if (this.$props.type == \"1\") {\r\n                this.filename = \"json/tutorMenu.json\"\r\n            } else {\r\n                this.filename = \"json/adminMenu.json\"\r\n            }\r\n\r\n            this.menuList = loadfile(this.filename)\r\n        }\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n/deep/.el-submenu__title {\r\n    font-size: 18px;\r\n}\r\n\r\n.second-title {\r\n    font-size: 16px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}