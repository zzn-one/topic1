{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"TopicTask\",\n  data() {\n    return {\n      hasTask: true,\n      year: 3022,\n      hasTopic: false,\n      taskName: \"选题任务名1\",\n      dialogVisible: false\n    };\n  },\n  methods: {\n    topicSelect() {\n      this.$router.push(\"/home/topicSelect\");\n    },\n    handleClose(done) {\n      this.$confirm('确认关闭？').then(_ => {\n        done();\n      }).catch(_ => {});\n    },\n    openTopic() {\n      this.dialogVisible = true;\n    }\n  }\n};","map":{"version":3,"mappings":";AA2CA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA,uBACAC;QACAC;MACA,GACAC;IACA;IACAC;MACA;IACA;EAEA;AACA","names":["name","data","hasTask","year","hasTopic","taskName","dialogVisible","methods","topicSelect","handleClose","then","done","catch","openTopic"],"sourceRoot":"src/components/home/student","sources":["TopicTask.vue"],"sourcesContent":["<template>\r\n    <div>\r\n\r\n        <!-- 如果学生已有课题，显示 -->\r\n        <template v-if=\"hasTopic\">\r\n            <div>\r\n                <h1>{{ year }}届毕业设计选题</h1>\r\n                <h2>已结束</h2>\r\n            </div>\r\n        </template>\r\n        <!-- 如果学生没有课题 且 没任务 ，显示 -->\r\n        <template v-else-if=\"!hasTopic & !hasTask\">\r\n            <div>\r\n                <h1>{{ year }}届毕业设计选题</h1>\r\n                <h2>尚未开始</h2>\r\n            </div>\r\n        </template>\r\n        <template v-else>\r\n            <div>\r\n                <h2>{{ taskName }}</h2>\r\n                <h1>院系</h1>\r\n                <h1>专业</h1>\r\n                <h1>开始时间</h1>\r\n                <h1>选题时长</h1>\r\n                <el-row>\r\n                    <el-button type=\"primary\" plain @click=\"openTopic\">预览课题</el-button>\r\n                    <el-button type=\"success\" plain @click=\"topicSelect\">进入选题</el-button>\r\n                </el-row>\r\n            </div>\r\n        </template>\r\n\r\n        <!-- 预览课题页面 -->\r\n        <el-dialog title=\"预览课题页面\" :visible.sync=\"dialogVisible\" width=\"30%\" :before-close=\"handleClose\">\r\n            <span>这是预览课题页面</span>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"dialogVisible = false\">确 定</el-button>\r\n            </span>\r\n        </el-dialog>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: \"TopicTask\",\r\n    data() {\r\n        return {\r\n            hasTask: true,\r\n            year: 3022,\r\n            hasTopic: false,\r\n            taskName: \"选题任务名1\",\r\n            dialogVisible: false\r\n        }\r\n    },\r\n    methods: {\r\n        topicSelect() {\r\n            this.$router.push(\"/home/topicSelect\")\r\n        },\r\n        handleClose(done) {\r\n            this.$confirm('确认关闭？')\r\n                .then(_ => {\r\n                    done();\r\n                })\r\n                .catch(_ => { });\r\n        },\r\n        openTopic(){\r\n            this.dialogVisible=true\r\n        }\r\n\r\n    }\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}