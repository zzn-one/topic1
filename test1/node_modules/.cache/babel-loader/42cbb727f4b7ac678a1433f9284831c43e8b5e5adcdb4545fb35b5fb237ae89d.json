t(id);\n      }\n    }\n  },\n  created() {\n    //发送请求获取所有学生的信息\n    this.tutorList = [];\n\n    // 整点假数据\n    for (var i = 1; i <= 20; i++) {\n      let id = 303029320200;\n      this.tutorList.push({\n        tId: id + i + '',\n        tName: \"啊啊啊\",\n        title: '讲师',\n        major: \"计算机科学与技术\",\n        graduationYear: \"3030\",\n        phone: \"28038473629\",\n        email: \"2358472938@pp.com\"\n      });\n    }\n\n    //根据 管理员的所属学院 发送请求 获取专业列表\n    this.getCollegeById();\n  }\n};","map":{"version":3,"mappings":";AAkFA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;QACAC;QACAC;QACAC;MACA;MACAC;MACAC;MACAC;MACAL;MAEA;MACAM;MACAC;MACAC;MACAC;IAEA;EACA;EACAC;IACA;IACAC;MACA;MACA;;MAEA;MACA;MAEA;IACA;IACA;IACAC;MACA;MACA;MACA;MAEA;IACA;IACA;IACAC;MACA;MACAC;IAEA;IACA;IACAC,cAEA;IACA;IACAC,cAEA;IACA;IACAC,cAEA;IAEAC;MACA;IACA;IACA;IACAC;MACA;IACA;IACA;IACAC;MACA;IACA;IACA;IACA;MACA;MACA;QACA;MACA;IACA;IACA;IACA;MACA;MACA;MACA;QACA;QACA;MACA;IACA;EAEA;EACAC;IACA;IACA;;IAEA;IACA;MACA;MACA,oBACA;QACArB;QACAC;QACAqB;QACApB;QACAqB;QACAC;QACAC;MACA,EACA;IACA;;IAEA;IACA;EACA;AACA","names":["name","components","DialogTutorInfo","data","infoDialogVisible","searchForm","tId","tName","major","tutorList","majorList","formType","total","currentPage","pageSize","pageSizeList","methods","openInfoAdd","openInfoUpdate","deleteStuInfo","console","searchInfo","importInfo","exportInfo","closeInfoDialog","handleSizeChange","handleCurrentChange","created","title","graduationYear","phone","email"],"sourceRoot":"src/components/home/admin","sources":["InfoTutor.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <el-card class=\"box-card\">\r\n            <div slot=\"header\" class=\"card_header\">\r\n                <el-row>\r\n                    <el-col :span=\"12\">\r\n                        <div class=\"header_title\">\r\n                            <span>导师信息</span>\r\n                        </div>\r\n                    </el-col>\r\n\r\n                  